{
  "name": "rust-wasm",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/rust-wasm/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/packages/rust-wasm",
        "index": "packages/rust-wasm/src/public/index.html",
        "baseHref": "/",
        "main": "packages/rust-wasm/src/public/index.js",
        "tsConfig": "packages/rust-wasm/tsconfig.app.json",
        "assets": [],
        "styles": [],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "packages/rust-wasm/webpack.config.ts"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "packages/rust-wasm/webpack.config.ts"
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "rust-wasm:build",
        "hmr": true,
        "port": 4202
      },
      "configurations": {
        "development": {
          "buildTarget": "rust-wasm:build:development"
        },
        "production": {
          "buildTarget": "rust-wasm:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/rust-wasm/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "rust-wasm:build",
        "watch": false,
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "rust-wasm:build:development"
        },
        "production": {
          "buildTarget": "rust-wasm:build:production"
        }
      }
    }
  },
  "tags": []
}
